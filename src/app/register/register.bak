<div class="content-wrapper page">
  <div class="content custom-scrollbar">
    <div id="login" class="p-8 ">
      <div class="auth overflow custom-scrollbar">
        <mat-horizontal-stepper linear="false" class="container mt-5" #stepper>
          <mat-step>
            <div id="logo" class="row text-center">
              <div>
                <!--<img class="" src="images/1/logoRMS.png" width="150" height="80">-->
                <img src="../../assets/rms logo.png" height="100" />
              </div>
            </div>
            <br />

            <!--	<h2 class="fs-title"  Style="border-bottom:2px solid deepskyblue"  border-sizing="100px" align="center"><br/>Sign in with RMS</h2>
                                    <hr Style="border-bottom:2px solid deepskyblue" style="color:#cfdaeb" width="13%" class="col-lg-pull-4" >
                                    <hr Style="border-bottom:2px solid lightgray;color:#cfdaeb" width="13%" align="center" >-->

            <div class="row">
              <div class="col-md-4"></div>
              <div class="col-md-4">
                <hr
                  Style="border-bottom:2px solid deepskyblue;color:#cfdaeb;padding-left: 200px;padding-right: 100px"
                  width="13%"
                />
              </div>
              <div class="col-md-4"></div>
            </div>

            <h3 class="fs-title" align="center">Sign in with RMS</h3>

            <!-- Login-->
            <div class="row">
              <div class="col-6" style="height:350px;">
                <div class="card">
                  <div class="card-header">
                    I'm already an RMS customer
                  </div>
                  <div class="card-body">
                    <h5 class="card-title"></h5>

                    <form [formGroup]="loginForm" (ngSubmit)="login(stepper)">
                      <div class="m-3">
                        <input
                          class="form-control"
                          name="email"
                          id="emailLogin"
                          type="email"
                          placeholder="Email address"
                          formControlName="email"
                          required
                        />
                      </div>

                      <div class="m-3">
                        <input
                          class="form-control"
                          id="password"
                          type="password"
                          placeholder="Password"
                          formControlName="motpass"
                          required
                        />
                      </div>

                      <!--<input type="submit" value="Login">-->
                      <input
                        type="submit"
                        class="btn btn-primary m-3 "
                        value="Log in"
                      />
                    </form>
                  </div>
                </div>
              </div>

              <!-- CreateAccount-->
              <div class="col-md-6" style="height:350px;">
                <div class="card">
                  <div class="card-header">
                    I'm new to RMS
                  </div>
                  <div class="card-body">
                    <h5 class="card-title"></h5>

                    <div class="pull-left" style="padding-left:20px">
                      <p>Create an RMS account for :</p>
                    </div>
                    <br />
                    <br />
                    <div style="padding-left:60px">
                      <ul>
                        <li class="pull-left">Manage your services</li>
                        <br />
                        <li class="pull-left">
                          Manage your orders in progress
                        </li>
                        <br />
                        <li class="pull-left">
                          Manage your bills and payment methods
                        </li>
                        <br />
                      </ul>
                      <br />
                    </div>

                    <button
                      type="button"
                      class="btn btn-primary m-3 "
                      matStepperNext
                    >
                      Add Company
                    </button>
                  </div>
                </div>
              </div>
            </div>

            <!-- Forgot Password-->
            <div id="divForgotPassword" style="display: none;">
              <div id="container-login">
                <br />
                <div class="form__field">
                  <input
                    class="form-control"
                    id="emailPwdForgot"
                    type="email"
                    style="width:55%"
                    placeholder="Email address"
                    required
                  />
                </div>
                <br />
                <input
                  type="submit"
                  class="btn btn-info prism zeroclip"
                  style="padding: 10px 50px;color:black"
                  value="Send"
                />
                <br />
                <br />
                <p>
                  <a href="#">
                    <span class="glyphicon glyphicon-edit"></span> Return</a
                  >
                </p>
              </div>
            </div>

            <!-- <div class="mt-5" style="float: right;margin-top: 80px;">
              <button mat-stroked-button color="primary" matStepperPrevious>
                Previous
              </button>

              <button
                mat-flat-button
                color="primary"
                class="mx-2"
                matStepperNext
                [disabled]="!isUserLoggedIn"
              >
                Next
              </button>
            </div> -->
          </mat-step>

          <mat-step>
            <h1 style="color:black;" class="fs-title text-center">
              <b>Personal Information</b>
            </h1>
            <form [formGroup]="personalInfo">

              <div class="form-group">
                <label for="name">Name</label>
                <input
                  type="text"
                  class="form-control"
                  id="name"
                  formControlName="name"
                  required
                />
              </div>

              <div class="form-group">
                <label for="email">Email address</label>
                <input
                id="email"
                  type="email"
                  class="form-control"
                  formControlName="email"
                  required
                />
              </div>

              <div class="form-group">
                <label for="activity">Activity</label>
                <input
                  type="text"
                  class="form-control"
                  id="activity"
                  formControlName="activity"
                  required
                />
              </div>

              <div class="form-group">
                <label for="sector">Sector</label>
                <input
                  type="text"
                  class="form-control"
                  id="sector"
                  formControlName="sector"
                  required
                />
              </div>

              <div class="form-group">
                <label for="description">Description</label>
                <textarea
                  type="text"
                  class="form-control"
                  id="description"
                  formControlName="description"
                  required
                ></textarea>
              </div>

              
            </form>
            <div class="mt-5" style="float: right;margin-top: 80px;">
              <button mat-stroked-button color="primary" matStepperPrevious>
                Previous
              </button>

              <button
                mat-flat-button
                color="primary"
                class="mx-2"
                matStepperNext
                [disabled]="personalInfo.invalid"
              >
                Next
              </button>
            </div>
          </mat-step>

          <mat-step>
            <h1 class="text-center mt-5 text-bold text-black">
              Number Of Users
            </h1>

            <div class="row mt-5">
              <div class="col-6">
                <h3>choose your number of users</h3>
              </div>

              <div class="col-6">
                  <select name="" id="" class="form-control" [formControl]="nbOfUsers" required>
                    <option value="6"
                      ><p>
                        6 Users
                        <span>20 DT</span>
                      </p></option
                    >
                    <option value="15"
                      >15 Users
                      
                      <span>40 DT</span></option
                    >
                    <option value="50"
                      >50 Users
                      
                      <span>65 DT</span></option
                    >
                    <option value="0"
                      >Unlimited users
                      
                      <span>80 DT</span>
                    </option>
                  </select>

              </div>
            </div>

            <div class="mt-5" style="float: right;margin-top: 80px;">
              <button mat-stroked-button color="primary" matStepperPrevious>
                Previous
              </button>

              <button
              mat-flat-button
              color="primary"
              matStepperNext
              class="mt-5"
              style="float: right;"
              [disabled]="nbOfUsers.invalid"
            >
              Next
            </button>
            </div>
            
          </mat-step>

          <mat-step>
            <h1 class="text-center mt-5 text-bold text-black">
              Users
            </h1>
            <p *ngIf="quotaExceeded" style="color: red;">Number of users has exceeded the quota</p>
            <form [formGroup]="addUserForm" class="form-inline" (ngSubmit)="addUser()">
              <label class="sr-only" for="inlineFormInputName1">Name</label>
              <input type="text" class="form-control mb-2 mr-sm-2" id="inlineFormInputName1" placeholder="Name" formControlName="name" required>
            
              <label class="sr-only" for="inlineFormInputName2">Name</label>
              <input type="text" class="form-control mb-2 mr-sm-2" id="inlineFormInputName2" placeholder="Email" formControlName="email" required>

              <label class="sr-only" for="inlineFormInputName3">Name</label>
              <select class="form-control mb-2 mr-sm-2" id="inlineFormInputName3" formControlName="role" required>
                <option selected>Type</option>
                <option value="admin">Admin</option>
                <option value="manager">Manager</option>
                <option value="editor">Editor</option>
                <option value="viewer">Viewer</option>
              </select>
            
              <button type="submit" class="btn btn-primary mb-2" [disabled]="addUserForm.invalid">ADD</button>
            </form>
            <mat-list>
              <div mat-subheader>Admins</div>
              <mat-list-item *ngFor="let a of admin">
                <mat-icon mat-list-icon>account_box</mat-icon>
                <div mat-line>{{a.name}}</div>
                <div mat-line> {{a.email}} </div>
              </mat-list-item>
              <mat-divider></mat-divider>
              <div mat-subheader>Managers</div>
              <mat-list-item *ngFor="let m of manager">
                <mat-icon mat-list-icon>account_box</mat-icon>
                <div mat-line>{{m.name}}</div>
                <div mat-line> {{m.email}} </div>
              </mat-list-item>
              <div mat-subheader>Editors</div>
              <mat-list-item *ngFor="let e of editor">
                <mat-icon mat-list-icon>account_box</mat-icon>
                <div mat-line>{{e.name}}</div>
                <div mat-line> {{e.email}} </div>
              </mat-list-item>
              <div mat-subheader>Viewers</div>
              <mat-list-item *ngFor="let v of viewer">
                <mat-icon mat-list-icon>account_box</mat-icon>
                <div mat-line>{{v.name}}</div>
                <div mat-line> {{v.email}} </div>
              </mat-list-item>
            </mat-list>
            <div class="mt-5" style="float: right;margin-top: 80px;">
              <button mat-stroked-button color="primary" matStepperPrevious>
                Previous
              </button>

              <button
                mat-flat-button
                color="primary"
                class="mx-2"
                matStepperNext
                [disabled]="staffcount == 0"
              >
                Next
              </button>
            </div>
          </mat-step>

